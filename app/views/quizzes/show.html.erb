  <!-- if user in students or teacher or assistant or admin -->
  <div class="page-inner">
      <div class="page-title">
        <h3><%= @quiz.name %></h3>

      </div>
      <div id="main-wrapper" ng-controller="quizzes">
        <div class="row">


          <div class="col-md-12">
            <div class="panel panel-success">
              <div class="panel-heading">
                <h3 class="panel-title"><%= @quiz.name %></h3>
              </div>
            </div>
          </div>


          <div class="col-lg-12 col-md-12">
            <div class="panel panel-white">
              <div class="panel-heading">
                <h4 class="panel-title">Alumnos del quiz</h4>
              </div>
              <div class="panel-body">
                <div class="table-responsive project-stats">
                  <table class="table">
                    <thead>
                    <tr>
                      <th>Rut</th>
                      <th>Nombre</th>
                      <th>Apellido</th>
                      <th>Email</th>
                      <th>Nota</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% @quiz.users_students.each do |user| %>
                        <tr>
                          <td><%= user.rut%></td>
                          <td><%= user.name%></td>
                          <td><%= user.lastname %></td>
                          <td><%= user.email%></td>
                          <td>:)</td>
                          <td>
                          </td>
                        </tr>
                    <% end %>

                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>





        </div><!-- Row -->
      </div><!-- Main Wrapper -->
      <div class="page-footer">
        <p class="no-s">2016 &copy; Clickapp.</p>
      </div>
    </div><!-- Page Inner -->
